<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="qsoft.hacktbilisi.app">

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
        <activity
                android:name=".actvities.LoginActivity"
                android:label="@string/app_name"
                android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
                android:name=".actvities.ChooseUniversityActivity"
                android:label="@string/app_name"
                android:screenOrientation="portrait">
        </activity>

        <activity
                android:name=".actvities.ChooseGroupActivity"
                android:label="@string/app_name"
                android:screenOrientation="portrait">
        </activity>

        <activity
                android:name=".actvities.ChooseTermActivity"
                android:label="@string/app_name"
                android:screenOrientation="portrait">
        </activity>

        <activity
                android:name=".actvities.ImportCreateActivity"
                android:label="@string/app_name"
                android:screenOrientation="portrait">
        </activity>

        <activity
                android:name=".actvities.ActivityScheduleDay"
                android:label="@string/app_name"
                android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
